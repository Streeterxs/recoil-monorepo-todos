{
  "name": "@StreeterxsTodos/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "yarn run relay && react-native start",
    "build": "yarn run relay && react-native build",
    "relay": "yarn run relay-compiler --schema ./schema/schema.graphql --src ./src/ --watchman false $@ --language typescript"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-native": "0.62.2",
    "@StreeterxsTodos/relay":"^0.0.1",
    "react-relay": "^0.0.0-experimental-94e87455",
    "relay-compiler-language-typescript": "^12.0.3",
    "relay-runtime": "^9.1.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-relay": "^9.1.0",
    "eslint": "^6.5.1",
    "graphql": "^14.4.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.11.0",
    "relay-compiler": "^9.1.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
