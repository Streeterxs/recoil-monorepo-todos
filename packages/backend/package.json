{
  "name": "@StreeterxsTodos/backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "babel": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\"",
    "build": "npm run clear && babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts\" --ignore *.spec.js --out-dir dist --copy-files",
    "clear": "rimraf ./dist",
    "database:clear:users": "npm run babel ./src/scripts/clearUsers",
    "prestart": "npm run build",
    "start": "nodemon .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Afonso Araujo Neto",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-async-generator-functions": "7.8.3",
    "@babel/plugin-transform-async-to-generator": "7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-flow": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
