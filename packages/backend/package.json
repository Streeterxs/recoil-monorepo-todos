    "graphql-subscriptions": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.12.0",
    "koa-graphql": "^0.8.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "mongoose": "^5.9.7",
    "nodemon": "^2.0.2",
    "subscriptions-transport-ws": "^0.9.16",
    "tslint": "^6.1.1"
  }
}
